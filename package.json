{
  "name": "sosse",
  "amdName": "sosse",
  "version": "0.1.0",
  "description": "Spice your servers up with a lil Sosse",
  "source": "lib/main.ts",
  "main": "dist/main.js",
  "module": "dist/main.m.js",
  "umd:main": "dist/main.umd.js",
  "esmodule": "dist/main.modern.js",
  "exports": {
    ".": {
      "browser": "./dist/main.m.js",
      "umd": "./dist/main.umd.js",
      "import": "./dist/main.m.js",
      "require": "./dist/main.js"
    },
    "./dev-socket-client": {
      "browser": "./dev-socket-client/dist/main.m.js",
      "umd": "./dev-socket-client/dist/main.umd.js",
      "import": "./dev-socket-client/dist/main.m.js",
      "require": "./dev-socket-client/dist/main.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "types": "dist/main.d.ts",
  "authors": [
    "Katja Lutz <mail@katjalutz.ch>"
  ],
  "homepage": "https://github.com/sossejs/sosse",
  "repository": {
    "type": "git",
    "url": "https://github.com/sossejs/sosse.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.11",
    "@types/ws": "^7.2.5",
    "badgen": "^3.1.0",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "mallery": "1.0.0-alpha.7",
    "otion": "^0.3.2",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.1.9",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.1.0",
    "tsconfig-paths": "^3.9.0"
  },
  "dependencies": {
    "chokidar": "^3.4.0",
    "clear-module": "^4.1.1",
    "cuid": "^2.1.8",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.15",
    "microbundle": "^0.12.2",
    "strip-ansi": "^6.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "ws": "^7.3.0"
  },
  "files": [
    "lib",
    "dist",
    "example/package.json",
    "example/lib",
    "example/dist",
    "preact/package.json",
    "preact/lib",
    "preact/dist",
    "otion/package.json",
    "otion/lib",
    "otion/dist",
    "dev-socket-client/package.json",
    "dev-socket-client/lib",
    "dev-socket-client/dist"
  ],
  "standard-version": {
    "scripts": {
      "postchangelog": "node scripts/badges.js && node scripts/site.js && git add assets site README.md"
    }
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "json-summary"
    ],
    "coveragePathIgnorePatterns": [
      "main.(?:tsx|ts|jsx|js)",
      "/lib/tpl-lib/",
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{ts,tsx,js,jsx}",
      "example/lib/**/*.{ts,tsx,js,jsx}",
      "preact/lib/**/*.{ts,tsx,js,jsx}",
      "otion/lib/**/*.{ts,tsx,js,jsx}",
      "dev-socket-client/lib/**/*.{ts,tsx,js,jsx}"
    ]
  }
}
