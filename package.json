{
  "name": "sosse",
  "amdName": "sosse",
  "version": "0.8.0-next.10",
  "description": "Composable toolkit for (p)react full-stack node web servers",
  "source": "lib/main.ts",
  "main": "dist/main.js",
  "bin": "cli/dist/main.js",
  "exports": {
    ".": {
      "require": "./dist/main.js"
    },
    "./dev-socket-client": {
      "browser": "./dev-socket-client/dist/main.mjs",
      "umd": "./dev-socket-client/dist/main.umd.js",
      "import": "./dev-socket-client/dist/main.mjs",
      "require": "./dev-socket-client/dist/main.js"
    },
    "./uni": {
      "browser": "./uni/dist/main.mjs",
      "umd": "./uni/dist/main.umd.js",
      "import": "./uni/dist/main.mjs",
      "require": "./uni/dist/main.js"
    },
    "./react": {
      "browser": "./react/dist/main.mjs",
      "umd": "./react/dist/main.umd.js",
      "import": "./react/dist/main.mjs",
      "require": "./react/dist/main.js"
    },
    "./preact": {
      "browser": "./preact/dist/main.mjs",
      "umd": "./preact/dist/main.umd.js",
      "import": "./preact/dist/main.mjs",
      "require": "./preact/dist/main.js"
    },
    "./cli": {
      "browser": "./cli/dist/main.mjs",
      "umd": "./cli/dist/main.umd.js",
      "import": "./cli/dist/main.mjs",
      "require": "./cli/dist/main.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "types": "dist/main.d.ts",
  "authors": [
    "Katja Lutz <mail@katjalutz.ch>"
  ],
  "homepage": "https://github.com/sossejs/sosse",
  "repository": {
    "type": "git",
    "url": "https://github.com/sossejs/sosse.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.5",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.13",
    "@types/ws": "^7.4.0",
    "badgen": "^3.2.2",
    "chai": "^4.2.0",
    "colorette": "1.2.1",
    "cuid": "^2.1.8",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "mallery": "1.0.0-alpha.7",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "sade": "^1.7.4",
    "serve-static": "^1.14.1",
    "sockette": "^2.0.6",
    "standard-version": "^9.0.0",
    "strip-ansi": "^6.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "preact-render-to-string": "^5.1.12",
    "module-alias": "^2.2.2"
  },
  "optionalDependencies": {
    "chokidar": "^3.4.3",
    "microbundle": "^0.12.4",
    "otion": "^0.6.2",
    "preact": "^10.5.7",
    "ws": "^7.4.1"
  },
  "_moduleAliases": {
    "sosse": "."
  },
  "files": [
    "lib",
    "dist",
    "example/package.json",
    "example/lib",
    "example/dist",
    "preact/package.json",
    "preact/lib",
    "preact/dist",
    "otion/package.json",
    "otion/lib",
    "otion/dist",
    "dev-socket-client/package.json",
    "dev-socket-client/lib",
    "dev-socket-client/dist",
    "uni/package.json",
    "uni/lib",
    "uni/dist",
    "react/package.json",
    "react/lib",
    "react/dist",
    "cli/package.json",
    "cli/lib",
    "cli/dist"
  ],
  "standard-version": {
    "scripts": {
      "postchangelog": "node scripts/badges.js && node scripts/site.js && git add assets site README.md"
    }
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "json-summary"
    ],
    "coveragePathIgnorePatterns": [
      "main.(?:tsx|ts|jsx|js)",
      "/lib/tpl-lib/",
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{ts,tsx,js,jsx}",
      "example/lib/**/*.{ts,tsx,js,jsx}",
      "preact/lib/**/*.{ts,tsx,js,jsx}",
      "dev-socket-client/lib/**/*.{ts,tsx,js,jsx}",
      "uni/lib/**/*.{ts,tsx,js,jsx}",
      "react/lib/**/*.{ts,tsx,js,jsx}",
      "cli/lib/**/*.{ts,tsx,js,jsx}"
    ]
  }
}
